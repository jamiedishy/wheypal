    </div>\n          <ul class=\"list-highlight\">\n            <li class=\"my-2\">\n              <custom-input\n                v-model=\"name\"\n                label=\"Name\"\n                placeholder=\"Name\"\n              ></custom-input>\n            </li>\n            <li class=\"my-2\">\n              <custom-input\n                v-model=\"email\"\n                label=\"Email\"\n                placeholder=\"placeholder@placeholder.ca\"\n              ></custom-input>\n            </li>\n            <li class=\"my-2\">\n              <custom-input\n                v-model=\"password\"\n                label=\"Password\"\n                placeholder=\"Password\"\n              ></custom-input>\n            </li>\n          </ul>\n          <div slot=\"footer\">\n            <div class=\"row ml-1\">\n              <div class=\"col-md-4\">\n                <custom-button @click=\"signUp()\" class=\"mb-1\" color=\"primary\">\n                  Sign up\n                </custom-button>\n                <br />\n                Already a member?\n                <br />\n                <router-link to=\"/signIn\">\n                  <custom-button class=\"mt-1\" color=\"primary\" outline>\n                    Sign in\n                  </custom-button>\n                </router-link>\n              </div>\n            </div>\n          </div>\n        </card>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\n// @ is an alias to /src\nimport {\n  Card,\n  Input,\n  Button,\n  Modal\n} from \"rbc-wm-framework-vuejs/dist/wm/components\";\nimport { mapActions, mapState } from \"vuex\";\nexport default {\n  name: \"Home\",\n  components: {\n    card: Card,\n    \"custom-input\": Input,\n    \"custom-button\": Button,\n    modal: Modal\n  },\n  data() {\n    return {\n      name: \"\",\n      email: \"\",\n      password: \"\",\n      modalIsOpen: false,\n      error: \"\",\n      image: \"cyclists.png\"\n    };\n  },\n  computed: {\n    ...mapState({\n      userId: state => state.wheypal.userId\n    })\n  },\n  methods: {\n    ...mapActions([\"createUser\", \"logoffUser\"]),\n\n    async signUp() {\n      this.error = \"\";\n      const body = {\n        name: this.name,\n        email: this.email,\n        password: this.password\n      };\n      if (body.name === \"\" || body.email === \"\" || body.password === \"\") {\n        this.error = \"Cannot sign up. Please enter all credentials.\";\n        this.modalIsOpen = !this.modalIsOpen;\n      } else {\n        try {\n          await this.createUser(body);\n          this.$router.push(\"/discover\");\n        } catch (e) {\n          this.error = e;\n          this.modalIsOpen = !this.modalIsOpen;\n        }\n      }\n    },\n    mounted() {\n      this.logoffUser(); // clear all states\n    }\n  }\n};\n</script>\n\n<style scoped>\nh1 {\n  font-size: 4rem;\n}\nh3 {\n  font-size: 2.074rem;\n}\n.header {\n  position: relative;\n  text-align: center;\n}\n</style>\n"]}]}