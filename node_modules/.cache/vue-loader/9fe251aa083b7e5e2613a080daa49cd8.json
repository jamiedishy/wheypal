r\"\n              :show-button=\"false\"\n              @toggle=\"modalIsOpen = false\"\n            >\n              <p>{{ this.error }}</p>\n            </modal>\n\n            <div class=\"row mb-4\">\n            <header class=\"col-md-4 mb-3 ml-1\">\n              <h1 style=\"font-size: 4rem;\" class=\"mt-5\">Discover</h1>              \n            </header>\n            <img class=\"col-md-6\" src=\"../assets/discover.png\">\n            </div>\n\n            <div class=\"row\">\n              <div\n                class=\"col-md-6\"\n                v-for=\"(recommendation, index) in userRecommendations\"\n                :key=\"index\"\n              >\n                <card class=\"mb-3\">\n                  <div slot=\"header\">\n                    <h3>\n                      Name:\n                      {{\n                        recommendation.name ? recommendation.name : \"Undefined\"\n                      }}\n                    </h3>\n                  </div>\n                  <ul class=\"list-highlight\">\n                    <li class=\"my-2\">\n                      Email:\n                      {{\n                        recommendation.email\n                          ? recommendation.email\n                          : \"Undefined\"\n                      }}\n                    </li>\n                  </ul>\n                  <div slot=\"footer\">\n                    <custom-button\n                      class=\"mr-2\"\n                      icon=\"thumbs-up\"\n                      @click=\"swipeRight()\"\n                      outline\n                    >\n                    </custom-button>\n                    <custom-button\n                      class=\"mr-2\"\n                      icon=\"thumbs-down\"\n                      @click=\"swipeLeft()\"\n                      outline\n                    >\n                    </custom-button>\n                  </div>\n                </card>\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>\n    </layout>\n  </div>\n</template>\n\n<script>\nimport {\n  Layout,\n  Card,\n  Button,\n  Modal\n} from \"rbc-wm-framework-vuejs/dist/wm/components\";\nimport { mapActions, mapState } from \"vuex\";\nimport sideNav from \"../../sidenav.JSON\";\nexport default {\n  name: \"Discover\",\n  components: {\n    \"layout\": Layout,\n    \"card\": Card,\n    \"custom-button\": Button,\n    \"modal\": Modal\n  },\n  data() {\n    return {\n      sideNav: sideNav,\n      dynamicComponent: \"Discover\",\n      error: \"\",\n      modalIsOpen: false\n    };\n  },\n  computed: {\n    ...mapState({\n      userRecommendations: state => state.wheypal.userRecommendations,\n      userToken: state => state.wheypal.userToken,\n      userEmail: state => state.wheypal.userEmail\n    })\n  },\n  methods: {\n    ...mapActions([\"getRecommendations\"])\n  },\n  async created() {\n    this.error = \"\";\n    const body = {\n      userEmail: this.userEmail,\n      userToken: this.userToken\n    };\n    try {\n      await this.getRecommendations(body);\n    } catch (e) {\n      this.error = e;\n      this.modalIsOpen = !this.modalIsOpen;\n    }\n  }\n};\n</script>\n<style>\nh1 {\n  font-size: 3.074rem;\n}\n.discover {\n  padding-left: 5%;\n  padding-top: 0%;\n}\n</style>\n"]}]}