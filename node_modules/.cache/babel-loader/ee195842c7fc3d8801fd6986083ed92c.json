A;AAAA;AAAA;AACA,gBAAA,KAAA,CAAA,KAAA,GAAA,EAAA;AADA;AAAA;AAAA,uBAGA,KAAA,CAAA,UAAA,EAHA;;AAAA;AAIA,gBAAA,KAAA,CAAA,OAAA,CAAA,IAAA,CAAA,GAAA;;AAJA;AAAA;;AAAA;AAAA;AAAA;AAMA,gBAAA,KAAA,CAAA,KAAA;AACA,gBAAA,KAAA,CAAA,WAAA,GAAA,CAAA,KAAA,CAAA,WAAA;;AAPA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AASA;AAXA;AAzBA,CAAA","sourcesContent":["<template>\n  <div>\n    <layout\n      top-nav\n      logo=\"https://i.ibb.co/5GFxj30/handcuffs-2.png\"\n      :router=\"true\"\n      :side-nav=\"sideNav\"\n      toggle=\"full\"\n      class=\"profile\"\n    >\n      <div class=\"container\" slot=\"content\">\n        <div class=\"row\">\n          <div class=\"col-md-12\">\n            <modal\n              id=\"basicModal\"\n              :active=\"modalIsOpen\"\n              title=\"Error\"\n              :show-button=\"false\"\n              @toggle=\"modalIsOpen = false\"\n            >\n              <p>{{ this.error }}</p>\n            </modal>\n            <div class=\"row\">\n            <header class=\"col-md-3 ml-3\">\n              <h1 style=\"font-size: 4rem;\" class=\"mt-5\">Profile</h1>\n            </header>\n            <img class=\"col-md-8\" src=\"../assets/profile.png\">\n            </div>\n            <card>\n              <div slot=\"header\">\n                <h3>User preferences</h3>\n              </div>\n              <template v-if=\"editMode\">\n                <ul class=\"list-highlight\">\n                  <li class=\"my-2\">\n                    <custom-input\n                      label=\"Username\"\n                      placeholder=\"username\"\n                      v-model=\"username\"\n                    ></custom-input>\n                  </li>\n                  <li class=\"my-2\">\n                    <custom-input\n                      label=\"Password\"\n                      placeholder=\"password\"\n                    ></custom-input>\n                  </li>\n                </ul>\n                <div slot=\"footer\">\n                  <custom-button @click=\"editMode = !editMode\" color=\"primary\">\n                    Update\n                  </custom-button>\n                </div>\n              </template>\n              <template v-else>\n                <ul class=\"list-highlight\">\n                  <li class=\"my-2\">\n                    Name: {{ userName ? userName : \"Undefined\" }}\n                  </li>\n                  <li class=\"my-2\">\n                    Email: {{ userEmail ? userEmail : \"Undefined\" }}\n                  </li>\n                  <li class=\"my-2\">Id: {{ userId ? userId : \"Undefined\" }}</li>\n                  <li class=\"my-2\">\n                    Password: {{ userPassword ? userPassword : \"Undefined\" }}\n                  </li>\n                </ul>\n                <div slot=\"footer\">\n                  <custom-button @click=\"editMode = !editMode\" color=\"primary\">\n                    Edit\n                  </custom-button>\n                </div>\n              </template>\n            </card>\n            <custom-button @click=\"signOut()\" class=\"mt-2\" color=\"primary\">\n              Sign out\n            </custom-button>\n          </div>\n        </div>\n      </div>\n    </layout>\n  </div>\n</template>\n\n<script>\nimport {\n  Layout,\n  Card,\n  Button,\n  Input,\n  Modal\n} from \"rbc-wm-framework-vuejs/dist/wm/components\";\nimport { mapActions, mapState } from \"vuex\";\nimport sideNav from \"../../sidenav.JSON\";\nexport default {\n  name: \"Profile\",\n  components: {\n    \"layout\": Layout,\n    \"card\": Card,\n    \"custom-button\": Button,\n    \"custom-input\": Input,\n    \"modal\": Modal\n  },\n  data() {\n    return {\n      editMode: false,\n      sideNav: sideNav,\n      error: \"\",\n      modalIsOpen: false\n    };\n  },\n  computed: {\n    ...mapState({\n      userName: state => state.wheypal.userName,\n      userId: state => state.wheypal.userId,\n      userPassword: state => state.wheypal.userPassword,\n      userEmail: state => state.wheypal.userEmail\n    })\n  },\n  methods: {\n    ...mapActions([\"logOffUser\"]),\n    async signOut() {\n      this.error = \"\";\n      try {\n        await this.logOffUser();\n        this.$router.push(\"/\");\n      } catch (e) {\n        this.error = e;\n        this.modalIsOpen = !this.modalIsOpen;\n      }\n    }\n  }\n};\n</script>\n<style>\nh1 {\n  font-size: 4rem;\n}\n\n.profile {\n  padding-left: 5%;\n  padding-top: 0%;\n}\n</style>\n"],"sourceRoot":"src/views"}]}